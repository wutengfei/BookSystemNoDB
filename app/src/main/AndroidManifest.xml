<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.administrator.book">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".com.AndroidUI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".com.AndroidUI.student.StudentActivity">
        </activity>
        <activity android:name=".com.AndroidUI.student.InsertActivity">
        </activity>
        <activity android:name=".com.AndroidUI.student.UpdataActivity">
        </activity>
        <activity android:name=".com.AndroidUI.student.showAllStudentActivity">
        </activity>
    </application>

</manifest>
